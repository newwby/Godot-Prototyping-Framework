; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot Prototyping Framework"
config/description="Godot 4 conversion of the Godot Prototyping Framework.

Not all DDAT-GPF 3.5 features are currently available; features are being converted over one at a time."
config/version="1.0.0"
config/tags=PackedStringArray("plugin")
config/custom_user_dir_name="godot_prototyping_framework"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://gpf_icon.png"

[autoload]

GlobalLog="*res://addons/gpf_core/autoload/global_log.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=960
window/size/window_height_override=540

[editor]

script/templates_search_path="res://addons/gpf_core/templates"

[editor_plugins]

enabled=PackedStringArray("res://addons/gpf_core/plugin.cfg", "res://addons/gut/plugin.cfg")
